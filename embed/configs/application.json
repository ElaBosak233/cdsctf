{
  "gin": {
    "cors": {
      "allow_methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "allow_origins": [
        "*"
      ]
    },
    "paths": {
      "media": "./media",
      "assets": "./assets",
      "frontend": "./dist"
    },
    "host": "0.0.0.0",
    "port": 8888
  },
  "container": {
    "provider": "docker",
    "docker": {
      "uri": "unix:///var/run/docker.sock",
      "public_entry": "127.0.0.0"
    },
    "k8s": {
      "namespace": "default",
      "path": {
        "config": "./k8s-config.yml"
      }
    }
  },
  "db": {
    "provider": "sqlite3",
    "postgres": {
      "dbname": "cloudsdale",
      "host": "127.0.0.1",
      "username": "cloudsdale",
      "password": "cloudsdale",
      "port": 5432,
      "sslmode": "disable"
    },
    "mysql": {
      "dbname": "cloudsdale",
      "host": "127.0.0.1",
      "username": "cloudsdale",
      "password": "cloudsdale",
      "port": 3306
    },
    "sqlite3": {
      "filename": "db.sqlite3"
    }
  },
  "email": {
    "address": "",
    "password": "",
    "smtp": {
      "host": "",
      "port": 0
    }
  },
  "jwt": {
    "expiration": 180,
    "secret_key": "20101010"
  }
}